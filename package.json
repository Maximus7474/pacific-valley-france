{
  "name": "pacific-valley-organiser",
  "version": "0.4.0",
  "description": "Discord bot built for the french branch of the Pacific Valley Project, to simplify the organisation of game sessions",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "clear-commands": "node scripts/register_commands.js --clear-commands",
    "build": "node scripts/build.js",
    "deploy": "node scripts/register_commands.js",
    "dev-deploy": "npm run build && npm run deploy",
    "start": "node dist/index.js",
    "setup-db": "node scripts/setup_database.js"
  },
  "keywords": [
    "discordbot",
    "discord.js",
    "typescript"
  ],
  "author": "Maximus7474",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "discord.js": "^14.20.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.15.32",
    "better-sqlite3": "^12.2.0",
    "eslint": "^9.29.0",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
}
